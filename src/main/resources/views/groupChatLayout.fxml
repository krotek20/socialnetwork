<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="501.0" prefWidth="689.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="socialnetwork.ui.gui.controllers.GroupChatController">
   <children>
      <ListView fx:id="groupChatUsersList" layoutX="48.0" layoutY="101.0" onMouseClicked="#handleUserSelection" prefHeight="261.0" prefWidth="263.0" stylesheets="@../css/ListViewStyle.css" />
      <Button fx:id="createGroupChatButton" layoutX="311.0" layoutY="442.0" mnemonicParsing="false" onMouseClicked="#handleCreateGroupChat" prefHeight="26.0" prefWidth="68.0" style="-fx-background-color: #ea5800;" text="Create" textFill="WHITE">
         <font>
            <Font name="Calibri" size="12.0" />
         </font></Button>
      <Label layoutX="65.0" layoutY="14.0" text="ADD PARTICIPANTS">
         <font>
            <Font name="System Bold" size="24.0" />
         </font></Label>
      <TextField fx:id="filterUsersTextField" layoutX="65.0" layoutY="58.0" onKeyTyped="#filterUserList" prefHeight="25.0" prefWidth="229.0" promptText="Filter" stylesheets="@../css/TextFieldStyle.css" />
      <TextField fx:id="groupNameTextField" layoutX="436.0" layoutY="58.0" promptText="Name your Group" stylesheets="@../css/TextFieldStyle.css" />
      <ListView fx:id="addedUsersList" layoutX="379.0" layoutY="101.0" onMouseClicked="#handleGroupUserSelection" prefHeight="261.0" prefWidth="263.0" stylesheets="@../css/ListViewStyle.css" />
      <Button fx:id="addToGroupChatButton" layoutX="150.0" layoutY="389.0" mnemonicParsing="false" onMouseClicked="#addToGroupList" prefHeight="25.0" prefWidth="59.0" style="-fx-background-color: #ea5800;" text="Add" textFill="WHITE">
         <font>
            <Font name="Calibri" size="12.0" />
         </font>
      </Button>
      <Button fx:id="removeFromGroupChatButton" layoutX="481.0" layoutY="389.0" mnemonicParsing="false" onMouseClicked="#removeFromGroupList" style="-fx-background-color: #ea5800;" text="Remove" textFill="WHITE" />
   </children>
</AnchorPane>
